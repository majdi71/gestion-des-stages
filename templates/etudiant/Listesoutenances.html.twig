{% extends 'user.html.twig' %}

{% block title %} Gestion de stage{% endblock %}

{% block body %}
  <div class="container-fluid">
 <!-- Page Heading -->
<div class="d-sm-flex align-items-center justify-content-between mb-4">
<h1 class="h3 mb-0 text-gray-800">Liste des soutenances</h1>
                       
</div>

 <!-- Content Row -->
<div class="row">
   <div class="container-fluid">
  <table class="table">
 
     <thead class="thead-light">
     
      <tr>
       <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">

      <th scope="col">Class</th>
      <th scope="col">Nom </th>
      <th scope="col">Membre du jury</th>
      <th scope="col">Date de soutenance</th>
        <th scope="col">Salle</th>
      <th scope="col">Note soutenance</th>
        <th scope="col"></th>
    
      
    </tr>
  </thead>
  <tbody>
   {% for soutenance in soutenances %}
    <tr>
  
    <td>{{ soutenance.class }}</td>
    <td>{{ soutenance.nom }}</td>
      <td>{{ soutenance.jury }}</td>
    <td>{{ soutenance.date }}</td>
    <td>{{ soutenance.salle }}</td>
    <td>{{ soutenance.note }}</td>
    </tr>
     {% endfor %}
  </tbody>
</table>
                    </div>
                    
              </div>     
 </div>
{% endblock %}
