{% extends 'user.html.twig' %}

{% block title %} Gestion de stage{% endblock %}

{% block body %}
  <div class="container-fluid">
 <!-- Page Heading -->
                    <div class="d-sm-flex align-items-center justify-content-between mb-4">
                        <h1 class="h3 mb-0 text-gray-800">Liste des entreprises</h1>
                       
                    </div>

                    <!-- Content Row -->
                    <div class="row">
                       <div class="container-fluid">
                    <table class="table">
 
   
 
   
  

   
     <thead class="thead-light">
     
      <tr>
   

      <th scope="col">ID</th>
      <th scope="col">Nom de l'entreprise</th>
      <th scope="col">Adresse</th>
      <th scope="col">Email entreprise</th>
        <th scope="col">Téléphone</th>
      <th scope="col">Spécialité(s)</th>
        <th scope="col"></th>
    
      
    </tr>
  </thead>
  <tbody>
   {% for entreprise in entreprises %}
    <tr>
  
    <td>{{ entreprise.id }}</td>
    <td>{{ entreprise.nom }}</td>
      <td>{{ entreprise.adresse }}</td>
    <td>{{ entreprise.email }}</td>
    <td>{{ entreprise.telephone }}</td>
    <td>{{ entreprise.Specialite }}</td>
    <td> <div class="items-link items-link2 f-right">
    <a href="{{ path('detentreprise',{'id':entreprise.id})}}">Voir plus</a>
                                      
                                    </div></td>
    </tr>
     {% endfor %}
  </tbody>
</table>
                    </div>
                    
                   </div>
 </div>
{% endblock %}
